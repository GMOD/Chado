>     feature_id AS polypeptide_tungsten_ion_contact_site_id,
>     feature.*
>   FROM
>     feature INNER JOIN cvterm ON (feature.type_id = cvterm.cvterm_id)
>   WHERE cvterm.name = 'polypeptide_tungsten_ion_contact_site';
