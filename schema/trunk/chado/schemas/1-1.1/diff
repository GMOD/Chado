>     feature INNER JOIN cvterm ON (feature.type_id = cvterm.cvterm_id)
>   WHERE cvterm.name = 'target_site_duplication' OR cvterm.name = 'CRISPR' OR cvterm.name = 'direct_repeat';
> 
