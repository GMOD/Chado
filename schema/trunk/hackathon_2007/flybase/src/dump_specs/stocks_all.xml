<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE chado SYSTEM "/users/zhou/work/flybase/xml/chado_stan.dtd">

<chado dumpspec="stocks_all.xml" date="Nov 13 2006">
    <stock dump="cols">
        <uniquename test="ms">^FBst[0-9]+$</uniquename>
        <is_obsolete test="yes">false</is_obsolete>
        
        <type_id>
            <cvterm dump="select">
                <cv_id>
                    <cv>
                        <name />
                    </cv>
                </cv_id>
                <name />
                <is_obsolete />
            </cvterm>
        </type_id>

        <!-- Get organism info -->
        <organism_id>
            <organism/>
        </organism_id>
        
        <stockcollection_stock>
            <stockcollection_id>
                <stockcollection>
                    <stockcollectionprop />
                </stockcollection>
            </stockcollection_id>
        </stockcollection_stock>

        <stock_genotype>
            <genotype_id>
                <genotype />
            </genotype_id>
        </stock_genotype>
        
        <stock_cvterm>
            <cvterm_id>
                <cvterm />
            </cvterm_id>
        </stock_cvterm>

    </stock>
</chado>
