<?xml version="1.0" encoding="UTF-8"?>

<chado dumpspec="synteny.xml" date="Fri Dec 16 2005">
    <feature dump="cols">
        <_appdata name="genus">$1</_appdata>
        <_appdata name="species">$2</_appdata>

        <is_obsolete test="yes">false</is_obsolete>

        <type_id test="yes">
            <cvterm>
                <name test="in">('orthologous_region','syntenic_region')</name>
            </cvterm>
        </type_id>

        <featureloc test="yes">
            <srcfeature_id>
                <feature>
                    <organism_id>
                        <organism>
                            <genus>$1</genus>
                            <species>$2</species>
                        </organism>
                    </organism_id>
                </feature>
            </srcfeature_id>
        </featureloc>

        <featureloc test="yes">
            <rank>0</rank>
            <srcfeature_id>
                <feature>
                    <is_obsolete>false</is_obsolete>
                </feature>
            </srcfeature_id>
        </featureloc>
        
        <featureloc test="yes">
            <rank>1</rank>
            <srcfeature_id>
                <feature>
                    <is_obsolete>false</is_obsolete>
                </feature>
            </srcfeature_id>
        </featureloc>

        <organism_id>
            <organism dump="cols" />
        </organism_id>

        <!-- Get the analysis program details. -->
        <analysisfeature dump="cols">
            <analysis dump="cols" />
        </analysisfeature>

        <featureloc dump="cols">
            <srcfeature_id>
                <feature dump="select">
                    <organism_id>
                        <organism dump="cols" />
                    </organism_id>
                    <type_id />
                    <uniquename />
                    <name />
                </feature>
            </srcfeature_id>
        </featureloc>


    </feature>
</chado>
